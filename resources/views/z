				<td width="150px">Biaya Sewa</td>
				<td width="10px" align="center">:</td>
					<?php
						$jumlah_waktu_penyewaan = date("d", strtotime($pesanan->tanggal_selesai)) - date("d", strtotime($pesanan->tanggal_mulai));
						$biaya_sewa = 1500000 * $jumlah_waktu_penyewaan
					?>
				<td  width="380px">Rp.{{$biaya_sewa}} </td>
			</tr>
			<tr>
				<td width="150px">Biaya Tambahan</td>
